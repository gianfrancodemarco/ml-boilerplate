name: tutorial

conda_env: conda.yaml

entry_points:
  download_database:
    command: "python src/data/download.py"
  main:
    parameters:
      alpha: {type: float, default: 0.6}
      l1_ratio: {type: float, default: 0.3}
    command: "python src/examples/sklearn_elasticnet_wine/models/train_model.py {alpha} {l1_ratio}"